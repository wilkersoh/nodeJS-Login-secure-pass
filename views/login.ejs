
<div class="login-wrap">
    <h2 class="login-title">Login</h2>
    <% include partials/messages %>
    <form class="login-form" action="/users/login" method="POST">
        <input class="login-input" type="text" placeholder="Email" name="email" autocomplete="off" />
        <input class="login-input" type="password" placeholder="Password" name="password" />
        <button type="submit" class="login-button">Login</button>
    </form>

    <a class="login-register login" href="/users/register">Don't have an accout? Register</a>
</div>
